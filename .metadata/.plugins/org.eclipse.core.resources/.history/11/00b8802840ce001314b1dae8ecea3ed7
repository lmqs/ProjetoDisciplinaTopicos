package br.com.projetoquestoesconcurso;

import android.app.Application;
import br.com.projetoquestoesconcurso.List.QuestoesList;

import com.russellcaughey.android.quiz.parts.QuizPlay;

public class QuestoesApplication extends Application {

	private QuestoesList currentQuiz;

	public void setCurrentQuiz(QuestoesList currentQuiz) {
		this.currentQuiz = currentQuiz;
	}

	public QuestoesList getCurrentQuiz() {
		return currentQuiz;
	}
}
